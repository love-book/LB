###################### 应用配置 ############################

# 监听端口
HttpAddr = 127.0.0.1
HttpPort = 8080

# 应用的模式，默认是 dev，为开发模式
RunMode = dev

# 是否开启 gzip 支持
EnableGzip = true

# 输出服务器名称
BeegoServerName = "app"

# 模板视图路径
ViewsPath = views

# 静态文件路径
StaticDir = static:static views:views

# 应用版本信息
AppVer = "1.0"

# 跨站请求伪造配置
enablexsrf = true
xsrfkey = 61oETzKXQAGaYdkL5gEmGeJJFuYh7EQnp2XdTP1o
xsrfexpire = 3600

#开启应用内文档开关
EnableDocs = true

# 获取 Request Body 里的 JSON 或 XML 的数据
copyrequestbody = true



###################### 数据库配置 ############################

