swagger: "2.0"
info:
  title: API文档
  description: 文档描述
  version: 1.0.0
  contact:
    email: 773683464@qq.com
basePath: /v1
paths:
  /user/index:
    post:
      tags:
      - user
      description: 获取用户列表
      operationId: UserController.获取用户列表
      parameters:
      - in: formData
        name: start
        description: 获取起始位置
        type: integer
        format: int64
      - in: formData
        name: length
        description: 获取分页步长
        type: integer
        format: int64
      - in: formData
        name: draw
        description: 获取请求次数
        type: integer
        format: int64
      responses:
        "200":
          schema:
            $ref: '#/definitions/models.User'
        "400":
          description: no enough input
        "500":
          description: 服务器错误!
  /user/insert:
    post:
      tags:
      - user
      description: 添加用户
      operationId: UserController.添加用户
      parameters:
      - in: formData
        name: nickname
        description: 昵称
        required: true
        type: string
      - in: formData
        name: openid
        description: openid
        type: string
      - in: formData
        name: wnickname
        description: 微信昵称
        type: string
      - in: formData
        name: wimgurl
        description: 微信头像
        type: string
      - in: formData
        name: mgurl
        description: 头像
        type: string
      - in: formData
        name: gender
        description: 性别1:男2:女3:保密
        type: integer
        format: int64
      - in: formData
        name: age
        description: 年龄
        type: integer
        format: int64
      - in: formData
        name: telphone
        description: 手机号
        type: integer
        format: int64
      - in: formData
        name: qq
        description: QQ
        type: string
      - in: formData
        name: weino
        description: 微博
        type: string
      - in: formData
        name: signature
        description: 个性签名
        type: string
      responses:
        "200":
          schema:
            $ref: '#/definitions/models.User'
        "400":
          description: no enough input
        "500":
          description: 服务器错误!
definitions:
  models.User:
    title: User
    type: object
    properties:
      address:
        type: string
      age:
        type: integer
        format: int32
      created_at:
        type: integer
        format: int64
      gender:
        type: integer
        format: int32
      imgurl:
        type: string
      nickname:
        type: string
      openid:
        type: string
      qq:
        type: string
      signature:
        type: string
      telphone:
        type: integer
        format: int32
      updated_at:
        type: integer
        format: int64
      userid:
        type: integer
        format: int64
      weibo:
        type: string
      wimgurl:
        type: string
      wnickname:
        type: string
tags:
- name: user
  description: |
    User API
